<launch>
  <arg
    name="gui"
    default="False" />
  <param
    name="use_gui"
    value="$(arg gui)" />
  <node
    name="qtui"
    pkg="qtui"
    type="qtui" />
  <node
    name="talker"
    pkg="fam"
    type="talker" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find model)/urdf.rviz" />
</launch>
